CREATE DATABASE secure_pay; 
CREATE TABLE `secure_pay`.`hashes` (`i` INT NOT NULL , `hash` CHAR(64) NOT NULL ) ENGINE = InnoDB; 

CREATE TABLE `secure_pay`.`transactions` (`time` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , `paise` BIGINT UNSIGNED NOT NULL ) ENGINE = InnoDB; 



